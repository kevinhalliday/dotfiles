{
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "semi": ["error", "never"],
    "no-unexpected-multiline": ["error"]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
